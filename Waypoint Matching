package ch.aero.RemoteControl;

public class Waypoint
{
  private String name;
	private float x;
	private float y;
	private float z;
	private float dx;
	private float dy;
	private float dz;
	private String comment;

	public Waypoint(String name, float x, float y,float z, float dx, float dy, float dz, String comment)
	{
		this.name = name;
		this.x = x;
		this.y = y;
		this.z = z;
		this.dx = dx;
		this.dy = dy;
		this.dz = dz;
		this.comment = comment;
	}

	public String getName(){ return name; }

	public float getX(){ return x; }

	public float getY(){ return y; }

	public float getZ(){ return z; }

	public float getDirX(){ return dx; }

	public float getDirY(){ return dy; }

	public float getDirZ(){ return dz; }

	public String getComment(){ return comment; }
}
